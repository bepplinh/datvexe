{
	"info": {
		"name": "Revenue API",
		"description": "Collection để test Revenue API endpoints",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "1. Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"if (pm.response.code === 200) {",
							"    var jsonData = pm.response.json();",
							"    pm.environment.set(\"token\", jsonData.access_token);",
							"    console.log(\"Token saved:\", jsonData.access_token);",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"email\": \"admin@example.com\",\n    \"password\": \"password\"\n}"
				},
				"url": {
					"raw": "{{base_url}}/api/login",
					"host": ["{{base_url}}"],
					"path": ["api", "login"]
				}
			}
		},
		{
			"name": "2. Dashboard - Day",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{token}}"
					},
					{
						"key": "Accept",
						"value": "application/json"
					}
				],
				"url": {
					"raw": "{{base_url}}/api/admin/revenue/dashboard?period=day",
					"host": ["{{base_url}}"],
					"path": ["api", "admin", "revenue", "dashboard"],
					"query": [
						{
							"key": "period",
							"value": "day"
						}
					]
				}
			}
		},
		{
			"name": "3. Dashboard - Month",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{token}}"
					},
					{
						"key": "Accept",
						"value": "application/json"
					}
				],
				"url": {
					"raw": "{{base_url}}/api/admin/revenue/dashboard?period=month&date=2024-01-15",
					"host": ["{{base_url}}"],
					"path": ["api", "admin", "revenue", "dashboard"],
					"query": [
						{
							"key": "period",
							"value": "month"
						},
						{
							"key": "date",
							"value": "2024-01-15"
						}
					]
				}
			}
		},
		{
			"name": "4. Trend - Daily",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{token}}"
					},
					{
						"key": "Accept",
						"value": "application/json"
					}
				],
				"url": {
					"raw": "{{base_url}}/api/admin/revenue/trend?period=day&from_date=2024-01-01&to_date=2024-01-31",
					"host": ["{{base_url}}"],
					"path": ["api", "admin", "revenue", "trend"],
					"query": [
						{
							"key": "period",
							"value": "day"
						},
						{
							"key": "from_date",
							"value": "2024-01-01"
						},
						{
							"key": "to_date",
							"value": "2024-01-31"
						}
					]
				}
			}
		},
		{
			"name": "5. Trend - Monthly",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{token}}"
					},
					{
						"key": "Accept",
						"value": "application/json"
					}
				],
				"url": {
					"raw": "{{base_url}}/api/admin/revenue/trend?period=month&from_date=2023-01-01&to_date=2024-01-31",
					"host": ["{{base_url}}"],
					"path": ["api", "admin", "revenue", "trend"],
					"query": [
						{
							"key": "period",
							"value": "month"
						},
						{
							"key": "from_date",
							"value": "2023-01-01"
						},
						{
							"key": "to_date",
							"value": "2024-01-31"
						}
					]
				}
			}
		},
		{
			"name": "6. Top Routes",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{token}}"
					},
					{
						"key": "Accept",
						"value": "application/json"
					}
				],
				"url": {
					"raw": "{{base_url}}/api/admin/revenue/top-routes?limit=10&from_date=2024-01-01&to_date=2024-01-31",
					"host": ["{{base_url}}"],
					"path": ["api", "admin", "revenue", "top-routes"],
					"query": [
						{
							"key": "limit",
							"value": "10"
						},
						{
							"key": "from_date",
							"value": "2024-01-01"
						},
						{
							"key": "to_date",
							"value": "2024-01-31"
						}
					]
				}
			}
		},
		{
			"name": "7. Top Trips",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{token}}"
					},
					{
						"key": "Accept",
						"value": "application/json"
					}
				],
				"url": {
					"raw": "{{base_url}}/api/admin/revenue/top-trips?limit=10&from_date=2024-01-01&to_date=2024-01-31",
					"host": ["{{base_url}}"],
					"path": ["api", "admin", "revenue", "top-trips"],
					"query": [
						{
							"key": "limit",
							"value": "10"
						},
						{
							"key": "from_date",
							"value": "2024-01-01"
						},
						{
							"key": "to_date",
							"value": "2024-01-31"
						}
					]
				}
			}
		}
	],
	"variable": [
		{
			"key": "base_url",
			"value": "http://localhost:8000",
			"type": "string"
		},
		{
			"key": "token",
			"value": "",
			"type": "string"
		}
	]
}


